@{
}

@* Using viewBag *@

<h1>Employee Details:</h1>
@foreach(var employee in ViewBag.Employees)
{
    
    <li>
        Id : @employee.EmployeeId, Name : @employee.EmployeeName , Age : @employee.EmployeeAge, Salary : @employee.EmployeeSalary
    </li>
}
